<h1>Clientes</h1>
<% link_to 'Novo cadastro', new_cliente_path %>
<table>
	<tr>
		<th>Nome</th>
		<th>CPF</th>
		<th>Email</th>
		<th>Endereco</th>
		<th>Data_de_Nascimento</th>
		<th></th>
	</tr>
	<% @clientes.each do |cliente|%>
		<tr>
		<td><%= cliente.Nome %></td>
		<td><%= cliente.CPF %></td>
		<td><%= cliente.Email %></td>
		<td><%= cliente.Endereco %></td>
		<td><%= cliente.Data_de_Nascimento %></td>
		<td><%= link_to 'Show', cliente_path(cliente) %></td>	
		</tr>
	<%end%>
	<%link_to 'Back', clientes_path %>
</table>
